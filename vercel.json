{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        },
        {
            "src": "clue-quest/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/index.py"
        },
        {
            "src": "/static/(.*)",
            "dest": "api/index.py"
        },
        {
            "src": "/media/(.*)",
            "dest": "api/index.py"
        },
        {
            "src": "/admin(.*)",
            "dest": "api/index.py"
        },
        {
            "src": "/(.*)",
            "dest": "/clue-quest/$1"
        }
    ]
}